{
    "name": "exp-semantic-skeleton",
    "description": "Semantic Skeleton for Expansive",
    "version": "0.1.0",
    "pak": ">=0.10",
    "keywords": [
        "expansive-plugin",
        "semantic"
    ],
    "licenses": "GPL",
    "dependencies": {
        "jquery": "~1.11",
        "semantic": "~0.50",
        "exp-js": "~0.2",
        "exp-less": "~0.2",
        "exp-css": "~0.2"
    },
    "export": [
        {
            "from": [
                "partials/**",
                "layouts/**",
                "source/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        }
    ],
    "blend": [
        "expansive.json"
    ],
    "mode": "debug"
}
