{
    "name": "exp-semantic-skeleton",
    "description": "Semantic Skeleton for Expansive",
    "version": "0.2.0",
    "keywords": [
        "expansive-plugin",
        "semantic"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-semantic-skeleton.git"
    },
    "dependencies": {
        "jquery": "~2",
        "semantic-ui": "~2",
        "exp-js": "~0.3",
        "exp-less": "~0.3",
        "exp-css": "~0.3",
        "exp-html": "~0.2"
    },
    "devDependencies": {
        "expansive": ">=0.6.0",
        "pak": ">=0.11.3"
    },
    "pak": {
        "blend": {
            "devDependencies": {
                "?esp": "~6.x"
            },
            "directories": {
                "?export": "contents/lib"
            },
            "pak": {
                "?mode": "debug",
                "?import": true
            }
        },
        "export": {
            "from": [
                "contents/**",
                "layouts/**",
                "partials/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        },
        "manage": "expansive.json"
    }
}
